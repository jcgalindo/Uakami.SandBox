{
    "template": {
                "id": "video_app",
                "width":"100%",
                "height":"100%",
                "backgroundColor":"#000000",
                "dite": {
                  "screenName": "",
                  "eventName": "loadDefault",
                  "eventData": ""
                },
                "elements": [{
                    "id": "app_contents",
                    "elements": [{
                        "id": "app_header",
                        "height": "10%",
                        "backgroundColor": "{{header_color}}",
                        "elements": [{
                            "id": "logo",
                            "left": "2%",
                            "width": "36%",
                            "backgroundImage": "{{app_header_image}}",
                            "backgroundColor": "{{header_color}}",
                            "backgroundSize": "contain",
                            "backgroundRepeat": "no-repeat"
                        }, {
                            "id": "share_this_button",
                            "right": "0",
                            "width": "30%",
                            "visible": "{{allow_sharing}}",
                            "backgroundImage": "{{share_this_image}}",
                            "backgroundSize": "contain",
                            "backgroundRepeat": "no-repeat",
                            "click": {
                                "fadeIn": ["share_this_screen"],
                                "dite": {
                                    "screenName": "",
                                    "eventName": "clickShare",
                                    "eventData": "shareThisHeader"
                                }
                            }
                        }]
                        },
                        {
                            "id": "video_player",
                            "height": "55%",
                            "width": "100%",
                            "top": "10%",
                            "backgroundImage": "{{poster_image}}",
                            "visible": true,
                            "widget": {
                                "widget": "youTubePlayer",
                                "options": {
                                    "id": "video_widget",
                                    "autoPlay":"{{autoplay_video}}",
                                    "videoEnd": {
                                        "fadeIn": ["{{video_end_command}}"],
                                        "fadeOut": ["video_player"]
                                    },
                                    "dite": {
                                        "screenName": ""
                                    },
                                    "videoUrl": "{{video_url}}",
                                    "videoId": "{{video_id}}",
                                    "showControls": "{{show_video_controls}}"
                                }
                            }
                        }
                        ,
                        {
                            "id": "video_thankyou",
                            "height": "224px",
                            "top": "42px",
                            "dite": {
                                "screenName": "thankYou",
                                "eventName": "loadThankYou",
                                "eventData": ""
                            },
                            "visible": false,
                            "backgroundImage": "{{poster_image}}",
                            "elements": [{
                                "id": "thankyou_blocker",
                                "backgroundGradient": {"colors": ["#000000"], "alphas": [0.8], "ratios": [127] },
                                "elements": [{
                                  "id": "large_thanks",
                                  "top": "20px",
                                  "height": "50px",
                                  "width": "250px",
                                  "left": "74px",
                                  "content": "{{thankyou_text_large}}",
                                  "fontSize": "20",
                                  "fontColor": "#FFFFFF",
                                  "textFormatting": {
                                      "align": "center"
                                  }
                                }, {
                                    "id": "small_thanks",
                                    "top": "70px",
                                    "height": "45px",
                                    "width": "350px",
                                    "left": "24px",
                                    "content": "{{thankyou_text_small}}",
                                    "fontSize": "14",
                                    "fontColor": "#FFFFFF",
                                    "textFormatting": {
                                        "align": "center"
                                    }
                                }, {
                                    "id": "play_again",
                                    "left": "94px",
                                    "top": "122px",
                                    "width": "211px",
                                    "height": "38px",
                                    "backgroundImage": "{{play_again_image}}",
                                    "click": {
                                        "show": ["video_player"],
                                        "hide": ["video_thankyou"],
                                        "dite": {
                                            "screenName": "thankYou",
                                            "eventName": "clickPlayPauseVideo",
                                            "eventData": "playVideo"
                                        }
                                    }
                                }, {
                                    "id": "share_widget",
                                    "visible": "{{show_thankyou_share}}",
                                    "top": "174px",
                                    "left": "32px",
                                    "widget": {
                                        "widget": "share",
                                        "options": {
                                            "id": "thankyou_share_widget",
                                            "dite": {
                                                "screenName": "thankYou"
                                            },
                                            "networks":[{
                                                "networks": ["facebook", "twitter", "googleplus", "tumblr", "linkedin", "blogger", "wordpress", "stumbleupon", "email"],
                                                "url": "{{share_url}}"
                                            }]
                                        }
                                    }
                                }]
                            }]
                        }, {
                                    "id": "audio_player",
                                    "width": "100%",
                                    "height": "35%",
                                    "top": "65%",
                                    "backgroundSize": "cover",
                                    "backgroundImage": "https://petestringfellow.moontoast.com/estore/file/thumbnail/id/719906/w/398/h/172",
                                    "elements": [
                                        {
                                            "id": "footer_button",
                                            "left": "59%",
                                            "width": "36%",
                                            "height": "80%",
                                            "top": "16%",
                                            "z": "9999",
                                            "click": {
                                                "link": "https://itunes.apple.com/us/album/pole-dance-single/id572006152"
                                            },
                                            "textFormatting": {
                                                "align": "center"
                                            }
                                        },
                                        {"id": "bottom_content",
                                        "width": "100%",
                                        "height": "100%",
                                        "top": "0",
                                        "left": ""
                                        }
                                    ]
                                    
                        }
                        ]
                }, {
                    "id": "share_this_screen",
                    "visible": false,
                    "dite": {
                        "screenName": "share",
                        "eventName": "loadShare",
                        "eventData": ""
                    },
                    "backgroundGradient": {"colors": ["#000000"], "alphas": [0.7], "ratios": [127] },
                    "elements": [{
                        "id": "main_pain",
                        "backgroundGradient": {"rotation": "-90", "colors": ["#bbbbbb", "#eeeeee", "#bbbbbb"], "alphas": [1, 1, 1], "ratios": [0, 127, 255] },
                        "border": {
                            "color": "#333333",
                            "thickness": "3"
                        },
                        "height": "85%",
                        "width": "95%",
                        "left": "15px",
                        "top": "62px",
                        "elements": [{
                            "id": "title_text",
                            "content": "Share",
                            "width": "75px",
                            "height": "25px",
                            "top": "42px",
                            "left": "10px",
                            "fontSize": "15"
                        }, {
                            "id": "flavor_title",
                            "content": "{{share_flavor}}",
                            "width": "300px",
                            "height": "25px",
                            "top": "77px",
                            "left": "10px",
                            "fontSize": "13",
                            "fontColor": "#444444"
                        }, {
                            "id": "share_row",
                            "height": "50px",
                            "top": "107px",
                            "left": "15px",
                            "widget": {
                                "widget": "share",
                                "options": {
                                    "id": "share_row_share_widget",
                                    "buttonsPerRow": "9",
                                    "dite": {
                                        "screenName": "share"
                                    },
                                    "networks":[{
                                        "networks": ["facebook", "twitter", "googleplus", "tumblr", "linkedin", "blogger", "wordpress", "stumbleupon", "email"],
                                        "url": "{{share_url}}"
                                    }]
                                }
                            }
                        }, {
                            "id": "add_store_label",
                            "left": "10px", 
                            "top": "170px",
                            "height": "25px",
                            "content": "{{embed_section_label}}",
                            "fontSize": "13",
                            "fontColor": "#444444"
                        }, {
                            "id": "embed_code_panel",
                            "left": "10px",
                            "top": "195px",
                            "height": "30px",
                            "width": "250px",
                            "backgroundGradient": {"rotation": "90", "colors": ["#eeeeee", "#dddddd"], "alphas": [1, 1], "ratios": [0, 255] },
                            "border": {
                                "color": "#cccccc"
                            },
                            "elements": [{
                                "id": "embed_code_content",
                                "top": "8px",
                                "content": "{{embed_code}}",
                                "fontSize": "13",
                                "textFormatting": {
                                    "selectable": true,
                                    "multiline": false
                                }
                            }]
                        }, {
                            "id": "embed_code_copy_panel",
                            "left": "270px",
                            "top": "195px",
                            "height": "30px",
                            "width": "90px",
                            "backgroundGradient": {"rotation": "90", "colors": ["#eeeeee", "#dddddd"], "alphas": [1, 1], "ratios": [0, 255] },
                            "border": {
                                "color": "#cccccc"
                            },
                            "click": {
                                "copyText": "{{embed_code}}",
                                "dite": {
                                    "screenName": "share",
                                    "eventName": "clickShareCopyLink",
                                    "eventData": "copyLink"
                                }
                            },
                            "elements": [{
                                "id": "copy_code_contents",
                                "top": "8px",
                                "content": "Copy Code",
                                "fontSize": "13",
                                "textFormatting": {
                                    "multiline": false,
                                    "align": "center"
                                }
                            }]
                        }, {
                            "id": "add_store_footer",
                            "left": "10px",
                            "top": "250px",
                            "height": "40px",
                            "width": "230px",
                            "content": "{{embed_code_footer}}",
                            "fontSize": "11",
                            "fontColor": "#444444"
                        }]
                    }, {
                        "id": "close_share_panel_button",
                        "backgroundImage": "{{close_button}}",
                        "top": "8%",
                        "left": "95%",
                        "width": "24px",
                        "height": "24px",
                        "click": {
                            "fadeOut": ["share_this_screen"]
                        }
                    }]
                }]
            },

    "templateParams": {
        
        "share_url": "https://toasters.moontoast.com/estore/embed/1810",
        "video_end_command": "video_thankyou",
        "video_url": "https://www.youtube.com/watch?v=5auCLKv81yM&feature=player_embedded",
        "video_id": "5auCLKv81yM",
        "show_video_controls": true,
        
        "header_color": "#324244",
        
        "app_header_image": "https://petestringfellow.moontoast.com/estore/file/thumbnail/id/720021/w/110/h/42",
