{
   "template":[
      {
         "id":"video_app",
         "width":"398px",
         "height":"460px",
         "backgroundColor":"#000000",
         "dite":{
            "eventType":"default",
            "eventData":"&lt;empty&gt;",
            "screenName":"&lt;empty&gt;",
            "eventName":"loadDefault"
         },
         "elements":[
            {
               "id":"app_contents",
               "elements":[
                  {
                     "id":"app_header",
                     "height":"42px",
                     "backgroundColor":"#000000",
                     "elements":[
                        {
                           "id":"logo",
                           "width":"200px",
                           "left":"6px",
                           "top":"5px",
                           "backgroundImage":"{{app_header_image}}",
                           "backgroundColor":"#000000"
                        },
                        {
                           "id":"share_this_button",
                           "visible":"{{allow_sharing}}",
                           "width":"172px",
                           "left":"332px",
                           "top":"15px",
                           "backgroundImage":"{{share_this_image}}",
                           "click":{
                              "fadeIn":[
                                 "share_this_screen"
                              ],
                              "dite":{
                                 "eventType":"share",
                                 "eventData":"shareThisHeader",
                                 "screenName":"&lt;empty&gt;",
                                 "eventName":"clickShare"
                              }
                           }
                        }
                     ]
                  },
                  {
                     "id":"video_player_loop",
                     "visible":true,
                     "height":"300px",
                     "width":"398px",
                     "top":"42px",
                     "widget":{
                        "widget":"videoPlayer",
                        "options":{
                           "id":"video_widget_loop",
                           "showControls":false,
                           "videoUrl":{
                              "flv":"{{loop_video_flv}}"
                           },
                           "loop":true,
                           "autoPlay":true
                        }
                     }
                  },
                  {
                     "id":"video_player",
                     "visible":true,
                     "height":"418px",
                     "width":"398px",
                     "top":"42px",
                     "widget":{
                        "widget":"videoPlayer",
                        "options":{
                           "id":"video_widget",
                           "showControls":false,
                           "videoEnd":{
                              "fadeOut":[
                                 "video_player"
                              ],
                              "fadeIn":[
                                 "linked_image"
                              ]
                           },
                           "autoPlay":true,
                           "dite":{
                              "eventData":true,
                              "screenName":"&lt;empty&gt;"
                           },
                           "videoUrl":{
                              "flv":"{{main_video_flv}}"
                           }
                        }
                     }
                  },
                  {
                     "id":"linked_image",
                     "visible":false,
                     "height":"172px",
                     "top":"344px",
                     "backgroundImage":"{{linked_image}}",
                     "elements":[
                        {
                           "left":"27px",
                           "height":"40px",
                           "width":"178px",
                           "id":"link1",
                           "top":"17px"
                        },
                        {
                           "id":"link2",
                           "top":"17px",
                           "width":"122px",
                           "height":"38px",
                           "left":"240px",
                           "click":{
                              "link":"http://clk.atdmt.com/MRT/go/444338209/direct;wi.1;hi.1/01/",
                              "dite":{
                                 "eventData":"hotspotLink1/value/{{creative_link}}",
                                 "screenName":"&lt;empty&gt;",
                                 "eventName":"&lt;empty&gt;"
                              }
                           }
                        }
                     ]
                  }
               ]
            },
            {
               "id":"share_this_screen",
               "visible":false,
               "backgroundGradient":{
                  "ratios":[
                     127
                  ],
                  "colors":[
                     "#000000"
                  ],
                  "alphas":[
                     0.7
                  ]
               },
               "elements":[
                  {
                     "id":"main_pain",
                     "width":"368px",
                     "height":"324px",
                     "left":"15px",
                     "top":"62px",
                     "border":{
                        "color":"#333333",
                        "thickness":"3"
                     },
                     "backgroundGradient":{
                        "rotation":"-90",
                        "ratios":[
                           0,
                           127,
                           255
                        ],
                        "colors":[
                           "#bbbbbb",
                           "#eeeeee",
                           "#bbbbbb"
                        ],
                        "alphas":[
                           1,
                           1,
                           1
                        ]
                     },
                     "elements":[
                        {
                           "id":"share_row",
                           "height":"50px",
                           "left":"15px",
                           "top":"107px",
                           "widget":{
                              "widget":"share",
                              "options":{
                                 "id":"share_row_share_widget",
                                 "buttonsPerRow":"9",
                                 "networks":[
                                    {
                                       "networks":[
                                          "facebook",
                                          "twitter",
                                          "googleplus",
                                          "tumblr",
                                          "linkedin",
                                          "blogger",
                                          "wordpress",
                                          "stumbleupon",
                                          "email"
                                       ],
                                       "url":"{{share_url}}"
                                    }
                                 ],
                                 "dite":{
                                    "screenName":"share"
                                 }
                              }
                           }
                        }
                     ]
                  },
                  {
                     "id":"close_share_panel_button",
                     "width":"24px",
                     "height":"24px",
                     "left":"368px",
                     "top":"54px",
                     "backgroundImage":"{{close_button}}",
                     "click":{
                        "fadeOut":[
                           "share_this_screen"
                        ]
                     }
                  }
               ]
            }
         ]
      }
   ],
   "templateParams":{
      "image_dir":"http://moontoast-assets-dev.s3.amazonaws.com/dturcotte/hungergames/images/",
      "video_dir":"http://moontoast-assets-dev.s3.amazonaws.com/dturcotte/hungergames/videos/",
      "share_url":"{{rootUrl}}",
      "creative_link":"http%3A%2F%2Fwww.beautyoftheweb.com%2Ftouchgallery%23%2Fcatchingfire",
      "header_color":"#fafafa",
      "share_icon_color":"#fafafa",
      "app_header_image":"{{image_dir}}videoApp_header.png",
      "linked_image":"{{image_dir}}videoApp_links.png",
      "share_this_image":"{{image_dir}}videoApp_share.png",
      "close_button":"{{image_dir}}close.png",
      "main_video_flv":"{{video_dir}}ie_hunger_games_700k.flv",
      "loop_video_flv":"{{video_dir}}fire_loop.flv",
      "allow_sharing":true
   }
}